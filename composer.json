{
  "name": "it-yakutia/yii2-gallery",
  "description": "Gallery module for Yii2",
  "type": "yii2-extension",
  "keywords": ["yii2", "extension", "gallery"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ayaal Kaplin",
      "email": "uraankhayayaal@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.0",
    "uraankhayayaal/yii2-sortable": "*",
    "uraankhayayaal/yii2-materializecomponents": "*",
    "uraankhayayaal/yii2-redactor": "*",
    "it-yakutia/yii2-blog": "*",
    "powerkernel/yii2-photoswipe": "*"
  },
  "autoload": {
    "psr-4": {
      "ityakutia\\gallery\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
